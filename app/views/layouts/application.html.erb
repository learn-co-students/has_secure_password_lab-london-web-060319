<!DOCTYPE html>
<html>
<head>
  <title>HasSecurePasswordLab</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<if flash[:errors]%> 
  <% flash[:errors].each do |error|%> 
    <%=error%> 
  <%end%> 
<%end%> 
<% if flash[:notice]%> 
    <%=flash[:notice]%> 
    <%end%> 

<%= yield %>

</body>
</html>
